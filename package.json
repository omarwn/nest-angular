{
  "name": "mean-starter",
  "version": "7.2.1",
  "license": "MIT",
  "author": "yafoxin",
  "description": "MEAN Stack Starter project built using Angular 2+, Express, Mongoose and Node.",
  "engines": {
    "node": "8.11.1",
    "npm": "6.4.0"
  },
  "scripts": {
    "ng": "ng",
    "build": "ng build --prod",
    "predev": "tsc -p server",
    "dev": "concurrently  \"ng serve --proxy-config proxy.conf.json --open\" \"tsc -w -p server/tsconfig.build.json\" \"nodemon dist/server/main.js\"",
    "prod": "concurrently \"ng build --aot --prod && tsc -p server/tsconfig.build.json && node dist/server/main.js\"",
    "e2e": "ng e2e",
    "build-nest": "tsc -p server/tsconfig.build.json",
    "format": "prettier --write \"src/**/*.ts\"",
    "start-nest": "ts-node -r tsconfig-paths/register server/src/main.ts",
    "start:dev": "concurrently --handle-input \"tsc -w -p server/tsconfig.build.json\" \"nodemon dist/server/main.js\" ",
    "prestart:prod": "rimraf dist && npm run build",
    "start:prod": "node dist/server/main.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.1.0",
    "@angular/common": "~8.1.0",
    "@angular/compiler": "~8.1.0",
    "@angular/core": "~11.0.5",
    "@angular/forms": "~8.1.0",
    "@angular/http": "~7.2.0",
    "@angular/platform-browser": "~8.1.0",
    "@angular/platform-browser-dynamic": "~8.1.0",
    "@angular/router": "~8.1.0",
    "@auth0/angular-jwt": "^2.1.0",
    "@nestjs/common": "^6.0.0",
    "@nestjs/core": "^6.0.0",
    "@nestjs/jwt": "^6.1.1",
    "@nestjs/mongoose": "^6.1.2",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.0.0",
    "@nestjs/platform-socket.io": "^6.5.3",
    "@nestjs/testing": "^6.5.0",
    "@nestjs/websockets": "^6.5.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "bootstrap": "^4.2.1",
    "core-js": "^2.6.5",
    "dotenv": "^8.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.6.4",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^5.0.1",
    "popper.js": "^1.14.6",
    "reflect-metadata": "^0.1.12",
    "rimraf": "^2.6.2",
    "rxjs": "~6.4.0",
    "rxjs-compat": "~6.3.3",
    "socket.io-client": "^2.2.0",
    "tether": "^1.4.7",
    "tslib": "^1.9.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.801.0",
    "@angular/cli": "~8.1.0",
    "@angular/compiler-cli": "~8.1.0",
    "@angular/language-service": "~8.1.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.0",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/mongoose": "^5.5.9",
    "@types/node": "~8.9.4",
    "@types/passport": "^1.0.0",
    "@types/passport-local-mongoose": "^4.0.12",
    "@types/socket.io": "^2.1.2",
    "chai-http": "^4.2.1",
    "codelyzer": "^5.0.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "node-sass": "^4.12.0",
    "nodemon": "^1.18.9",
    "prettier": "^1.15.3",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tsconfig-paths": "3.8.0",
    "tslint": "~5.15.0",
    "typescript": "~3.4.3",
    "wait-on": "^3.2.0"
  }
}
